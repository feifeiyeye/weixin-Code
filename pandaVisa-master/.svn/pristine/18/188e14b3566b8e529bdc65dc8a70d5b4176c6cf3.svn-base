/* 步骤 */
.step{    
    padding: 30rpx 50rpx;
    background: #f3f4f6;
}
.step_cont{
    display: flex;
    align-items: center;
    justify-content: space-around;
    text-align: center;
    position: relative;
}
.step_cont::after{
    content: "";
    position: absolute;
    top: 20rpx;
    left: 0;
    width: 100%;
    height: 1rpx;
    background: #e9e9e9;
    z-index: 1;
}
.step_li{
    background: #f3f4f6;
    z-index: 2;
    font-size: 30rpx;
}
.step_num{
    font-weight: bold;
    color: #f3f4f6;
    width: 40rpx;
    height: 40rpx;
    line-height: 40rpx;
    background: #dedede;
    border-radius: 50rpx;
    margin: 0 auto 20rpx;
}
.step_p{
    color: #aaa;
}
.step_li.li_on .step_num{
    background: #ff5a60;
}
.step_li.li_on .step_p{
    color: #ff5a60;
}

.pcolor{
  color: #cfcfcf;
}
