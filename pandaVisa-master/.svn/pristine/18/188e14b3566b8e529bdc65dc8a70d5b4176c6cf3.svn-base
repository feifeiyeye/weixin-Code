.triangle {
  width: 0;
  height: 0;
  position: absolute;
  border-left: 13rpx solid transparent;
  border-right: 13rpx solid transparent;
  border-top: 12rpx solid #fe5d62;
}
.pselect{
    position:relative;
    padding-right: 50rpx;
    overflow:hidden; 
    text-overflow:ellipsis; 
    white-space:nowrap; 
    box-sizing: border-box;
    min-height: 30px;
    line-height: 30px;
}
.pselect:after{
    content: " ";
    position: absolute;
    right: 20rpx;
    top: calc(50% - 6rpx);
    width: 0px;
    height: 0px;
    border: 12rpx solid transparent;
    border-top-color: #ff5a60; 
    display:inline-block;     
}

