/* checkbox样式 */
.check_li{
    display: inline-block;
}
.check_li_text {
    display: inline-block;
    vertical-align: middle;
}
.check_li_icon {
    position: relative;
    margin-right: 5rpx;
    display: inline-block;
    vertical-align: middle;
    box-sizing: border-box;
    width: 20px;
    height: 20px;
    border: 1rpx solid #ff5a60;
    border-radius: 50%;
}
.check_li_icon-checked {
    position: absolute;
    box-sizing: border-box;
    top: -2px;
    left: -2px;
    width: 20px;
    height: 20px;
    border-radius: 50%;
}
.check_li_bb .check_li_icon{
    width: 16px;
    height: 16px;
    margin-right: 20rpx;
}
.check_li_bb .check_li_icon-checked{
    top: -1px;
    left: -1px;
}
.check_li_bb .check_li_icon-checked:before{
    content: " ";
    position: absolute;
    width: 15px;
    height: 15px;
    background: #ff5a60;
    border: 1px solid #ff5a60;
    border-radius: 50%;
}
.check_li_bb .check_li_icon-checked:after{
    content: " ";
    position: absolute;
    top: 5px;
    left: 5px;
    width: 7px;
    height: 7px;
    background: #fff;
    border-radius: 50%;
}
.check_small .check_li_icon{
    width: 16px;
    height: 16px;
    margin-right: 10rpx;
}
.check_small .check_li_icon-checked{
    top: -1px;
    left: -1px;
}

